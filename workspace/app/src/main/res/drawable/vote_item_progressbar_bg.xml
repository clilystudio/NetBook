<?xml version="1.0" encoding="utf-8"?>
<layer-list
    xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <gradient
                android:startColor="#ffc6c6c6"
                android:endColor="#ffc6c6c6"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient
                    android:startColor="#ffb93221"
                    android:endColor="#ffb93221"/>
            </shape>
        </clip>
    </item>
</layer-list>